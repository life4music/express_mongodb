extends layout

block content
  h1= title

  form(method='get')
    input(type='text' value=search name='search')
    input(type='submit' value='Szukaj')
  each item in data 
    article 
      h1(class='title')=item.title
        span(class='date')=item.created.toLocaleString()
      p=item.description
